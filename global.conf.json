    "desc": "Remote Management (redfish and IPMI)",
    "path": "/home/ohollmen/hostrmgmt",
    "user": "mickey",
    "pass": "mouse",
    "enckey": "0000000000000000000000000000000000000000",
    "bootpin": "abc123",
    "debug": 1,
    "useipmi": 0,
    "userf": 0,
    "testurl": "http://localhost:3000/redfish/v1/Systems/1/nuc5"
  },
  "probe" : {
    "tout": 5000,
    "debug": 0
  },
  "docker": {
    "port": "4243",
    "config": "/home/ohollmen/docker.conf.json",
    "catalog": "/home/ohollmen/dockercat.conf.json",
    "hostgrp":"nuc"
  },
  "net": {
    "desc": "Installation Related Network Configuration",
    "netmask": "255.255.240.0",
